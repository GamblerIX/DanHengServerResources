{
  "OnInitSequece": [],
  "OnStartSequece": [
    {
      "TaskList": [
        {
          "$type": "RPG.GameCore.WaitGroupEvent",
          "EventName": {
            "Value": "OpenUI_MusicRhythm"
          },
          "OnEvent": [
            {
              "$type": "RPG.GameCore.ShowUI",
              "Name": "Ui.MusicRhythm.MusicRhythmMainEntrancePage",
              "WaitForExit": true,
              "OnUIEnter": [
                {
                  "$type": "RPG.GameCore.ExitTransitionLoadingUI",
                  "TaskEnabled": true,
                  "IsClientOnly": true
                },
                {
                  "$type": "RPG.GameCore.BlockInputController",
                  "ActionNameList": [],
                  "TaskEnabled": true,
                  "IsClientOnly": true
                },
                {
                  "$type": "RPG.GameCore.SetLocalPlayerDitherAlpha",
                  "TaskEnabled": true,
                  "IsClientOnly": true
                },
                {
                  "$type": "RPG.GameCore.LockPlayerControl",
                  "LockCameraControl": true,
                  "LockInteractSelect": true,
                  "TaskEnabled": true,
                  "IsClientOnly": true
                }
              ],
              "OnUIExitImmediately": [
                {
                  "$type": "RPG.GameCore.SetLocalPlayerDitherAlpha",
                  "DitherAlpha": 1.0,
                  "TaskEnabled": true,
                  "IsClientOnly": true
                },
                {
                  "$type": "RPG.GameCore.UnLockPlayerControl",
                  "UnLockCameraControl": true,
                  "TaskEnabled": true,
                  "IsClientOnly": true
                }
              ],
              "TaskEnabled": true,
              "IsClientOnly": true
            }
          ],
          "IsLoop": true,
          "TaskEnabled": true,
          "IsClientOnly": true
        }
      ]
    }
  ],
  "Type": "Group"
}